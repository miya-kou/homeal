$color-base: #000;
$color-yellow: #ffcd00;
$color-lightyellow: #fff5cc;
$color-orange: #ffac00;

// -----------------------------------------
// font
// -----------------------------------------

@font-face {
  font-family: "shuei";
  src: url(A-OTF-ShueiMGoStd-L.otf);
  font-weight: 400;
}

@font-face {
  font-family: "shuei";
  src: url(A-OTF-ShueiMGoStd-B.otf);
  font-weight: 700;
}

@font-face {
  font-family: "shueip";
  src: url(AP-OTF-ShueiNMGoStdN-B.otf);
  font-weight: 700;
}

@font-face {
  font-family: "avenir";
  src: url(AvenirNextRoundedPro-Bold.otf);
  font-weight: 700;
}

@font-face {
  font-family: "din";
  src: url(DINNextRoundedLTPro-Medium.otf);
  font-weight: 500;
}

@mixin shuei {
  font-family: "shuei", "Hiragino Kaku Gothic ProN", "ヒラギノ角ゴ ProN W3", YuGothic, "Yu Gothic", sans-serif;
}

@mixin shueip {
  font-family: "shueip", "Hiragino Kaku Gothic ProN", "ヒラギノ角ゴ ProN W3", YuGothic, "Yu Gothic", sans-serif;
}

@mixin avenir {
  font-family: "avenir", "Hiragino Kaku Gothic ProN", "ヒラギノ角ゴ ProN W3", YuGothic, "Yu Gothic", sans-serif;
}

@mixin din {
  font-family: "din", "Hiragino Kaku Gothic ProN", "ヒラギノ角ゴ ProN W3", YuGothic, "Yu Gothic", sans-serif;
}

@mixin c-font--english {
  font-family: "Helvetica Neue", "Arial", sans-serif;
}

// -----------------------------------------
// mixin
// -----------------------------------------

@mixin pc {
  @media screen and (min-width: 769px) {
    @content;
  }
}

@mixin normalpc {
  @media screen and (min-width: 769px) and (min-width: 1400px) {
    @content;
  }
}

@mixin middlepc {
  @media screen and (min-width: 769px) and (max-width: 1300px) {
    @content;
  }
}

@mixin smallpc {
  @media screen and (min-width: 769px) and (max-width: 1200px) {
    @content;
  }
}

@mixin smallestpc {
  @media screen and (min-width: 769px) and (max-width: 1100px) {
    @content;
  }
}

@mixin largetab {
  @media screen and (min-width: 769px) and (max-width: 1024px) {
    @content;
  }
}

@mixin tab {
  @media screen and (min-width: 769px) and (max-width: 900px) {
    @content;
  }
}

@mixin smalltab {
  @media screen and (min-width: 769px) and (max-width: 800px) {
    @content;
  }
}

@mixin sp {
  @media screen and (max-width: 768px) {
    @content;
  }
}

@mixin smallsp {
  @media screen and (max-width: 360px) {
    @content;
  }
}

@mixin largesp {
  @media screen and (min-width: 421px) and (max-width: 768px) {
    @content;
  }
}

@mixin landscapesp {
  @media screen and (min-width: 481px) and (max-width: 768px) {
    @content;
  }
}

@mixin large_landscapesp {
  @media screen and (min-width: 601px) and (max-width: 768px) {
    @content;
  }
}

// fontsize px → rem
$font-size: 16;

@function rem-calc($size, $font-size: $font-size) {
  $font-size: $font-size + 0px;
  $remSize: $size / $font-size;
  @return #{$remSize}rem;
}

/* ========================================
	Founfation
========================================= */

html {
  font-size: 100%;
  @include normalpc {
    font-size: 100%;
  }
  @include middlepc {
    font-size: 100%;
  }
  @include smallpc {
    font-size: 100%;
  }
  @include smallestpc {
    font-size: 100%;
  }
  @include largetab {
    font-size: 100%;
  }
  @include tab {
    font-size: 90%;
  }
  @include smalltab {
    font-size: 85%;
  }
  @include sp {
    font-size: 100%;
  }
  @include smallsp {
    font-size: 90%;
  }
  @include largesp {
    font-size: 100%;
  }
  @include landscapesp {
    font-size: 110%;
  }
  @include large_landscapesp {
    font-size: 120%;
  }
}

body {
  font-size: 1rem;
  font-family: "Hiragino Kaku Gothic ProN", "ヒラギノ角ゴ ProN W3", YuGothic, "Yu Gothic", sans-serif;
  color: rgba(0, 0, 0, 0.6);
/*   overflow: hidden; */
}

picture {
  display: block;
  width: 100%;
  height: auto;
}

img {
  display: block;
  max-width: 100%;
  height: auto;
}

a,
button {
  padding: 0;
  transition: 0.3s;
  cursor: pointer;
}

body, button, p, select, textarea,
.h1,.h2,.h3,.h4,.h5,.h6,h1,h2,h3,h4,h5,h6 {
  font-family: Roboto,"Noto Sans JP",Hiragino Sans,"ヒラギノ角ゴ ProN W3",Hiragino Kaku Gothic ProN,"メイリオ",Meiryo,sans-serif;
}

.is_scroll {
  @include sp {
    position: fixed;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

/* ========================================
	Component
========================================= */

.c-container {
  width: 100%;
  max-width: 925px;
  height: auto;
  margin: 0 auto;
  @include pc {
    width: 100%;
  }
  @include normalpc {
    width: 100%;
  }
  @include middlepc {
    width: 100%;
  }
  @include smallpc {
    width: 100%;
  }
  @include smallestpc {
    width: 100%;
  }
  @include largetab {
    width: 95%;
  }
  @include tab {
    width: 95%;
  }
  @include smalltab {
    width: 95%;
  }
  @include sp {
    width: 86.666667%;
  }
  @include smallsp {
    width: 86.666667%;
  }
  @include largesp {
    width: 86.666667%;
  }
  @include landscapesp {
    width: 86.666667%;
  }
  @include large_landscapesp {
    width: 86.666667%;
  }
}

.c-page__container {
  width: 100%;
  height: auto;
  padding: rem-calc(60px) 0;
  background-color: #fff;
  box-sizing: border-box;
  @include sp {
    padding: rem-calc(40px) 0;
  }
}

.c-pc_only {
  display: block !important;
  @include sp {
    display: none !important;
  }
}

.c-pc_only--flex {
  display: flex !important;
  @include sp {
    display: none !important;
  }
}

.c-sp_only {
  display: none !important;
  @include sp {
    display: block !important;
  }
}

/*----- margin -----*/

.c-mb--0 {
  margin-bottom: 0 !important;
}

.c-mb--30 {
  margin-bottom: 30px !important;
}

.c-mt--30 {
  margin-top: 30px !important;
}

.c-mb--40 {
  margin-bottom: 40px !important;
}

.c-mt--40 {
  margin-top: 40px !important;
}

/*----- animation -----*/

.c-slideup {
  -webkit-transform: translate3d(0, 3.125rem, 0);
  transform: translate3d(0, 3.125rem, 0);
  -webkit-transition: all 1s ease;
  transition: all 1s ease;
  opacity: 0;
}

.c-slideup.active {
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  opacity: 1;
}

/*----- bnr -----*/

.c-bnr {
  text-align: center;
  margin-bottom: 65px;
  &:hover {
    transition: .3s;
    opacity: 0.7;
  }
  @include sp {
    margin-bottom: 30px;
  }
}
.c-bnr__text {
  color: #000;
  margin-top: 20px;
  font-size: rem-calc(20px);
  font-weight: 700;
  @include sp {
    margin-top: 10px;
    font-size: rem-calc(14px);
  }
}

/*----- header -----*/
/* ここ　から*/
.c-header {
  position: fixed;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  z-index: 50;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: rem-calc(80px);
  padding: 0 2.5% 0 2.1%;
  background-color: #fff;
  box-sizing: border-box;
  // box-shadow: 0 rem-calc(20px) rem-calc(20px) rgba(0, 0, 0, 0.07);
  @include sp {
    top: 0;
    width: 100%;
    padding: 0 rem-calc(22px) 0 rem-calc(21px);
  }
  &.active {
    box-shadow: none;
  }
  &.announcebarIn{
    top: rem-calc(29px);
    @include sp{
      top: rem-calc(25px);
    }
  }
  &.top{
    top: 0;
  }
}
/* ここまで　*/

.c-header__logo {
  width: rem-calc(132px);
  height: auto;
}

.c-header__flexbox {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.c-header__navbox {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.c-header__nav {
  position: relative;
  transition: 0.3s;
  & a {
    padding: 0;
    text-decoration: none;
    font-size: rem-calc(14px);
    @include shueip;
    font-weight: 700;
    color: rgba(0, 0, 0, 0.87);
    line-height: 1;
  }
  &:not(:last-child) {
    margin-right: rem-calc(41px);
  }
  &:hover {
    &:after {
      content: "";
      position: absolute;
      bottom: rem-calc(-2px);
      left: 0;
      transform: translateY(100%);
      width: 100%;
      height: rem-calc(5px);
      border-radius: rem-calc(2.5px);
      background-color: $color-yellow;
    }
  }
}

.c-header__partition {
  width: rem-calc(3px);
  height: auto;
  margin: 0 rem-calc(39px) 0 rem-calc(28px);
}

.c-header__iconbox {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  @include sp {
    display: block;
  }
}

.c-header__iconbox_link {
  @include sp {
    display: flex;
    align-items: center;
    padding: 22px 0;
  }
}

.c-header__iconbox_text {
  display: none;
  @include sp {
    display: inline;
    padding-left: 10px;
    font-size: rem-calc(16px);
    @include shueip;
    font-weight: 700;
  }
}

.c-header__icon--kids {
  margin-right: rem-calc(31px);
  img {
    width: rem-calc(20px);
    height: auto;
    @include sp {
      display: inline;
      width: 25px;
    }
  }
}

.c-header__icon--mypage {
  margin-right: rem-calc(31px);
  img {
    width: rem-calc(18px);
    height: auto;
    @include sp {
      display: inline;
      width: 23px;
    }
  }
}

.c-header__icon--cart {
  position: relative;
  img {
    width: rem-calc(21px);
    height: auto;
    @include sp {
      display: inline;
      width: 25px;
    }
  }
}
.cart-link__bubble--visible {
  top: -8px;
  right: -6px;
  width: 12px;
  height: 12px;
  @include sp {
    top: 14px;
    left: 20px;
    right:auto;
  }
}

.c-menu_btn {
  @include sp {
    position: relative;
    width: 20px;
    height: 16px;
    display: inline-block;
    transition: all .4s;
    box-sizing: border-box;
    span {
      position: absolute;
      left: 0;
      width: 100%;
      height: 2px;
      background-color: #000;
      border-radius: 2px;
      display: inline-block;
      transition: all .4s;
      box-sizing: border-box;
      &:nth-of-type(1) {
        top: 0;
      }
      &:nth-of-type(2) {
        top: 7px;
      }
      &:nth-of-type(3) {
        bottom: 0;
      }
    }
    &.active {
      span {
          &:nth-of-type(1) {
            -webkit-transform: translateY(7px) rotate(-45deg);
            transform: translateY(7px) rotate(-45deg);
          }
          &:nth-of-type(2) {
            opacity: 0;
          }
          &:nth-of-type(3) {
            -webkit-transform: translateY(-7px) rotate(45deg);
            transform: translateY(-7px) rotate(45deg);
          }
      }
    }
  }
}

.c-menu {
  display: none;
  @include sp {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100vh;
    padding: 120px 30px 0;
    background: #fff;
    box-sizing: border-box;
    z-index: 99;
  }
}
.c-menu__link {
  @include sp {
    margin-bottom: 20px;
    padding-bottom: 20px;
    font-size: rem-calc(16px);
    @include shueip;
    font-weight: 700;
    border-bottom: 3px dotted $color_yellow;
    li {
      padding: 20px 0;
    }
  }
}
.c-announcement{
  min-height: 30px;
  background: #696969;
  color: #ffffff;
  font-weight: 700;
}
.announcement__text{
  font-size: 12px !important;
  letter-spacing:.05rem;
  padding: 5px 10px;
}
/*----- footer -----*/
.c-footer {
  width: 100%;
  height: auto;
  padding: rem-calc(50px) 0 rem-calc(25px);
  background-color: #ffffff;
  box-sizing: border-box;
  @include sp {
    padding: rem-calc(35px) 0 rem-calc(23px);
  }
}

.c-footer__message {
  margin: 0 auto rem-calc(21px);
  font-size: rem-calc(15px);
  font-weight: 300;
  color: rgba(0,0,0,.6);
  text-align: center;
  line-height: 1;
  @include sp {
    width: 89.333333%;
    margin: 0 auto rem-calc(14px);
    font-size: rem-calc(12px);
    text-align: left;
    line-height: 1.5;
  }
}

.c-footer__searchbox {
  display: flex;
  justify-content: center;
  width: 100%;
  height: auto;
  margin: 0 auto rem-calc(39px);
  @include sp {
    width: 89.333333%;
    justify-content: space-between;
    margin: 0 auto rem-calc(25px);
  }
}

.c-footer__inputarea {
  width: rem-calc(460px);
  height: rem-calc(40px);
  margin-right: rem-calc(11px);
  border-radius: rem-calc(20px);
  padding: 0 rem-calc(23px);
  background-color: #F5F5F5;
  box-sizing: border-box;
  font-size: rem-calc(14px);
  color: $color-base;
  @include sp {
    width: 71.641791%;
    margin-right: 0;
    padding: 0 rem-calc(15px);
  }
  &::placeholder {
    color: rgba(0, 0, 0, 0.2);
  }
}

.c-footer__submit {
  display: flex;
  justify-content: center;
  align-items: center;
  width: rem-calc(118px);
  height: rem-calc(40px);
  border-radius: rem-calc(20px);
  background-color: #813d26;
  font-size: rem-calc(14px);
  font-weight: 600;
  color: #fff;
  line-height: 1;
  cursor: pointer;
  transition: 0.3s;
  @include sp {
    width: 24.637681%;
  }
  &:hover {
    opacity: 0.7;
  }
}

.c-footer__flexbox {
  display: flex;
  justify-content: space-between;
  width: 89%;
  max-width: 1250px;
  height: auto;
  margin: 0 auto;
  @include sp {
    display: block;
    width: 89.333333%;
  }
}

.c-footer__linkboxes {
  padding-top: rem-calc(16px);
  box-sizing: border-box;
  @include sp {
    margin: 0 auto rem-calc(35px);
    padding-top: 0;
  }
}

.c-footer__links {
  display: flex;
  align-items: center;
  margin-bottom: rem-calc(17px);
  @include sp {
    margin-bottom: rem-calc(7px);
    flex-wrap: wrap;
  }
}

.c-footer__link {
  position: relative;
  & a {
    text-decoration: none;
    font-size: rem-calc(12px);
    font-weight: 400;
    color: rgba(0,0,0,.6);
    line-height: 1;
    white-space: nowrap;
    transition: 0.3s;
    @include sp {
      font-size: rem-calc(11px);
    }
    &:hover {
      opacity: 0.7;
    }
  }
  &:not(:last-child) {
    margin-right: rem-calc(28px);
  }
  &:after {
    content: "";
    position: absolute;
    top: 50%;
    right: rem-calc(-8px);
    transform: translate(100%, -35%);
    width: rem-calc(10px);
    height: rem-calc(14px);
    background: url(./icn-footer_link_partition.png) 50% 50% no-repeat;
    background-size: contain;
  }
  &.u-sp__after--dnone {
    @include sp {
      &:after {
        display: none;
      }
    }
  }
}

.c-footer__snsbox {
  @include sp {
    width: 97.014925%;
    margin: 0 auto;
  }
}

.c-footer__snslinks {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-bottom: rem-calc(23px);
  @include sp {
    justify-content: flex-start;
    margin-bottom: rem-calc(21px);
  }
}

.c-footer__snslink {
  &:not(:last-child) {
    margin-right: rem-calc(29px);
  }
  & a {
    display: block;
    height: auto;
    transition: 0.3s;
    &:hover {
      opacity: 0.7;
    }
    &.insta {
      width: rem-calc(25px);
    }
    &.twitter {
      width: rem-calc(31px);
    }
    &.line {
      width: rem-calc(26px);
    }
    &.fb {
      width: rem-calc(25px);
    }
  }
}

.c-footer__copyright {
  font-size: rem-calc(12px);
  color: rgba(0,0,0,.4);
  line-height: 1;
  letter-spacing: 0.03571em;
}
.header-right-sp_only{
  display: none;
  @include sp{
    display: flex !important;
    align-items: center;
    gap: 23px;
  }
}
/*----- button -----*/

.c-linkbtn {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: rem-calc(600px);
  height: rem-calc(80px);
  border-bottom: none!important;
  border-radius: rem-calc(40px);
  background-color: $color-yellow;
  box-sizing: border-box;
  margin: 0 auto;
  font-size: rem-calc(24px);
  font-weight: 600;
  color: $color-base;
  transition: 0.3s !important;
  &:hover {
    background-color: #e5b800;
  }
  &:after {
    content: none!important;
  }
  @include sp {
    flex-direction: column;
    width: 100%;
    height: rem-calc(100px);
    border-radius: rem-calc(50px);
    font-size: rem-calc(30px);
    line-height: 1;
    white-space: nowrap;
  }
  & .small {
    font-weight: 600;
    @include sp {
      display: block;
      margin-bottom: rem-calc(8px);
      font-size: rem-calc(16px);
      line-height: 1;
      white-space: nowrap;
    }
  }
  &:before {
    content: "";
    position: absolute;
    top: 50%;
    left: rem-calc(34px);
    transform: translateY(-60%);
    width: rem-calc(30px);
    height: rem-calc(30px);
    background: url(./icn_search_white.png) 50% 50% no-repeat;
    background-size: contain;
    @include sp {
      left: rem-calc(25px);
    }
  }
  &.size--medium {
    width: rem-calc(400px);
    height: rem-calc(60px);
    border-radius: rem-calc(30px);
    font-size: rem-calc(18px);
    @include sp {
      width: 100%;
      justify-content: center;
      flex-direction: initial;
      padding-bottom: 0;
    }
    &:before {
      display: none;
    }
  }
  &.white {
    background-color: #fff;
    &:before {
      background: url(./icn_search.png) 50% 50% no-repeat;
      background-size: contain;
    }
    &:hover {
      background-color: #fff5cc;
    }
  }
  &.continue_course{
    width: rem-calc(420px);
    height: rem-calc(60px);
    margin: 0 auto rem-calc(60px);
    font-size: rem-calc(20px);
    @include sp {
      width: rem-calc(330px);
      font-size: rem-calc(16px);
    }
  }
}

/*----- anchor -----*/

.c-anchor {
  margin-top: -130px;
  padding-top: 130px;
  @include sp {
    margin-top: -50px;
    padding-top: 50px;
  }
}

/*----- tag -----*/

.c-tag_new {
  position: absolute;
  top: 11px;
  right: 11px;
  width: 58px;
  height: 58px;
  line-height: 1;
  display: none;
  display: flex;
  align-items: center;
  justify-content: center;
  background: $color-orange;
  border: 4px solid #fff;
  border-radius: 50%;
  color: #fff;
  font-size: rem-calc(20px);
  @include avenir;
  z-index: 1;
}

/*----- price -----*/

.c-price_off {
  display: inline-block;
  margin: 5px 0;
  padding: 2px 15px;
  color: #fff;
  background: #FF8C00;
  border-radius: 8px;
  font-size: rem-calc(18px);
  font-weight: bold;
  @include sp {
    font-size: rem-calc(16px);
  }
  span {
    padding-right: 2px;
    //font-size: rem-calc(16px);
    font-weight: bold;
  }
}

