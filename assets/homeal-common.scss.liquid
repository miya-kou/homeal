/*----- menu -----*/
.menu {
  position: relative;
	width: 100%;
  margin: 0 auto;
  width: 100%;
  margin-bottom: rem-calc(15px);
  @include sp {
    margin-bottom: rem-calc(45px);
  }
}

.menu_header {
  @include shueip;
  display: flex;
  align-items: center;
  width: 100%;
  min-height: rem-calc(24px);
  margin: 0 auto rem-calc(3.5px);
  box-sizing: border-box;
  font-size: 18px;
  font-weight: 600;
  line-height: 1;
  color: rgba(0,0,0,0.87);
}

.menu_header.open{
  border-radius: 10px;
  text-decoration: none;
  cursor: pointer;
  position: relative;
  display: block;
  width: 100%;
  border: 1px solid  #E5E5E5;

  span{
    display: block;
    padding: 13px 30px 9px 11px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
  }

  &:after{
    content: "";
    position: absolute;
    right: 22px;
    top: 17px;
    border: solid black;
    border-width: 0 3px 3px 0;
    display: inline-block;
    padding: 3px;
    transform: rotate(45deg) translateY(-50%);
    -webkit-transform: rotate(45deg)  translateY(-50%);
  }
}
.menu_header.close {
  &:after{
    content: "";
    position: absolute;
    right: 15px;
    top: 15px;
    border: solid rgba(0,0,0,0.87);
    border-width: 0 3px 3px 0;
    display: inline-block;
    padding: 3px;
    transform: rotate(-90deg) translateY(-50%);
    -webkit-transform: rotate(-135deg)  translateY(-50%);
  }
}

.menu-content-wrapper{
  ul{
    z-index: 1;
    overflow: hidden;
    position: absolute;
    left: 0;
    right: 0;
    width: 100%;
    border-radius: 10px;
    max-height: 0;
    opacity: 0;

    transition: opacity 250ms, max-height 250ms;

    list-style: none;
    background: #ffffff;
    box-shadow: 0 0 20px 0 rgba(0,0,0,0.10), 0 20px 20px 0 rgba(0,0,0,0.15);

    li:last-child a{
      margin-bottom: 12px;
    }
  }
  li a.menu_header {
    padding: 14px 12px;
    margin: 0 auto 7px;
    &:hover{
      background-color: transparent;
    }
  }
  li a{
    display: block;
    padding: 11.3px 12px;
    text-decoration: none;
    cursor: pointer;
    transition: background-color 250ms, color 250ms;

    &:hover{
      background-color: #eeeeee;
    }
  }
  .menu__header {
    margin-bottom: 15px;
    padding: 10px 15px;
    font-size: 16px;
    &:hover{
       background-color: unset;
    }
  }
  .selected{
    font-weight: 700 !important;
    color: $color-yellow;
  }
}

.menu.open{
  .menu-content-wrapper ul{
    max-height: unset;
    overflow-y: auto;
    opacity: 1;
    position: relative;
  }
  .menu_header.open {
    display: none;
  }
}

