.p-page__title_container {
  width: 100%;
  height: auto;
  padding: rem-calc(123px) 0 0;
  background-color: $color-yellow;
  box-sizing: border-box;
  @include sp {
    padding: rem-calc(120px) rem-calc(15px) rem-calc(10px);
  }
  &[data-page="customers/login"],
  &[data-page="customers/register"]{
    padding: 0;
  }
  &[data-page="index"] {
    padding: 0;
  }
  &[data-page="product"] {
	padding: 0;
  }
  &[data-page="box_subscription"] {
